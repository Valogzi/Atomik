		"atomikjs": "^1.0.13",
		"cross-env": "^7.0.3",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsup": "^8.5.0",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"undici": "^7.12.0"
	},
	"dependencies": {
		"commander": "^14.0.0",
		"ws": "^8.18.3"
